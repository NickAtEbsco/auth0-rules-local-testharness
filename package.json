{
  "name": "auth0-rules-local-testharness",
  "version": "0.3.0",
  "description": "Deploy, execute, and test the output of Auth0 Rules using a webtask sandbox environment.",
  "author": "Sumana Malkapuram<sumana.malkapuram@auth0.com>",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:auth0/auth0-rules-local-testharness.git"
  },
  "keywords": [
    "auth0",
    "rules",
    "sandbox",
    "webtask",
    "testharness"
  ],
  "scripts": {
    "test": "cross-env AWS_ENV=local jest --watch",
    "clean": "rimraf lib",
    "build": "cross-env BABEL_ENV=production babel ./index.js --out-dir lib",
    "prepublishOnly": "npm run test && npm run clean && npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "assert": "^1.4.1",
    "auth0": "^2.9.1",
    "auth0-authz-rules-api": "^3.2.0",
    "axios15": "npm:axios@0.15.2",
    "cb": "^0.1.0",
    "ejs": "^2.5.5",
    "lodash": "^4.17.15",
    "request": "^2.79.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "chai": "^4.1.2",
    "jest": "^22.0.0",
    "rimraf": "^2.6.2",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.11.1"
  },
  "files": [
    "/lib/"
  ],
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "axios@0.15.2": "<rootDir>/node_modules/axios15",
      "axios@0.18.0": "<rootDir>/node_modules/axios18"
    },
    "moduleDirectories": [
      ".",
      "node_modules"
    ]
  }
}
